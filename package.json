{
  "name": "tokenizer-api",
  "version": "1.0.0",
  "description": "Tokenizer Credit card",
  "main": "serverless.yml",
  "scripts": {
    "dynamodb": "rimraf .dynamodb && serverless dynamodb install",
    "start": "rimraf .build && serverless offline start --stage dev --region us-east-2 --verbose",
    "test": "jest --location=global",
    "deploy:dev": "rimraf .build .serverless .warmup && serverless deploy --stage dev --region us-east-2 --verbose --aws-profile serverlessaccount",
    "remove:dev": "serverless remove --stage dev --region us-east-2 --verbose --aws-profile serverlessaccount"
  },
  "author": "ChanoHL",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.5.0",
    "@types/node": "^16.3.3",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.2.2",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@middy/core": "^3.6.0",
    "@middy/http-json-body-parser": "^3.6.0",
    "ajv": "^8.6.3",
    "ajv-errors": "^3.0.0"
  }
}
